{
  "apps": [
    {
      "name": "adbweb-web-django",
      "script": "manage.py",
      "args": ["runserver", "--settings=adbweb.settings.production", "0.0.0.0:8090"],
      "exec_mode": "fork",
      "instances": "1",
      "wait_ready": true,
      "autorestart": true,
      "max_restarts": 5,
      "interpreter": "python3"
    }
  ]
}
